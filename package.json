{
  "name": "node-architecture",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npx rimraf dist && babel . --ignore node_modules,dist,.git,.vscode --out-dir dist --copy-files",
    "start": "export NODE_ENV=production && node dist/app.js",
    "dev": "export NODE_ENV=development && nodemon --exec babel-node app.js",
    "stage": "npm run prestart && export NODE_ENV=staging && node dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:fix": "eslint --fix ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1277.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "eslint": "^8.32.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,vue,html,md}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json}": [
      "prettier --write",
      "git add"
    ]
  }
}
